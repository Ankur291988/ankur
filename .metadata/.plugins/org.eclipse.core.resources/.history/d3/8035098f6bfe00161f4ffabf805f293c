package utility;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

import utility.ExcelUtil;
public class Login {

	public static void main(String[] args) throws Exception {
		try{
		System.setProperty("webdriver.gecko.driver","C:\\Selenium\\geckodriver-v0.11.1-win64\\geckodriver.exe");
		WebDriver driver =new FirefoxDriver();
		String url = "http://store.demoqa.com/";
		driver.get(url);
		String Username = ExcelUtil.getCelldata(1,1);
		String Lastname = ExcelUtil.getCelldata(1,2);
		System.out.println(Username +" "+Lastname );
		
		
	}

}
